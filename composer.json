{
    "name": "astreya/settings-tool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "settings",
        "tool"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Astreya\\SettingsTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astreya\\SettingsTool\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astreya\\SettingsTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
